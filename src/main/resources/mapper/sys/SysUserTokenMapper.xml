<mapper namespace="com.fyqz.user.dao.SysUserTokenMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.fyqz.user.entity.SysUserToken">
		<result column="token" property="token" />
		<result column="expire_time" property="expireTime" />
